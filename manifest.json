{
  "name": "Wikipedia Wrapped",
  "description": "Track your time spent on Wikipedia",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "activeTab",
    "webNavigation"
  ],
  "host_permissions": [
    "https://*.wikipedia.org/*"
  ],
  "background": {
    "service_worker": "worker.js",
    "type": "module"
  }
}
